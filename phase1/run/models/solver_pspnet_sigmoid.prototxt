net: "models/train_pspnet_sigmoid.prototxt"
test_iter: 10
# make test net, but don't invoke it from the solver itself
test_interval: 999999999
display: 40
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-4
# high momentum
momentum: 0.9
# no gradient accumulation
iter_size: 1
max_iter: 300000
weight_decay: 0.0005
snapshot: 2000
snapshot_prefix: "snapshots/sigmoid/snapshot"
test_initialization: false
