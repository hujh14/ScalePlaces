net: "models/train_pspnet_specific.prototxt"
display: 5
average_loss: 20 # Display loss averaged over last x iterations
snapshot: 2000
snapshot_prefix: "snapshots/specific/snapshot"

base_lr: 1e-8
lr_policy: "fixed"
momentum: 0.5
weight_decay: 0.0005
iter_size: 1 # no gradient accumulation between iterations
max_iter: 300000

# Make a separate test net
test_initialization: false
test_iter: 10
test_interval: 999999999
